    "\n",
    "# 데이터 로드: 방글로르 주택 가격 데이터를 DataFrame으로 로드\n",
    "df1 = pd.read_csv(\"bengaluru_house_prices.csv\")\n",
    "\n",
    "# 데이터의 처음 몇 개 행 출력\n",
    "df1.head()\n",
    "\n",
    "# 데이터셋의 행과 열 개수 확인\n",
    "df1.shape\n",
    "\n",
    "# 데이터셋의 컬럼 목록 확인\n",
    "df1.columns\n",
    "\n",
    "# 'area_type' 열의 고유한 값 확인\n",
    "df1['area_type'].unique()\n",
    "\n",
    "# 'area_type' 열의 값 빈도 확인\n",
    "df1['area_type'].value_counts()\n",
    "\n",
    "# 불필요한 특성 제거\n",
    "df2 = df1.drop(['area_type','society','balcony','availability'], axis='columns')\n",
    "\n",
    "# 데이터셋의 새로운 차원 확인\n",
    "df2.shape\n",
    "\n",
    "# 데이터 정리: 결측치 처리\n",
    "df2.isnull().sum()\n",
    "\n",
    "# 결측치가 제거된 새로운 데이터셋 생성\n",
    "df3 = df2.dropna()\n",
    "\n",
    "# 결측치가 제거된 데이터셋의 차원 확인\n",
    "df3.shape\n",
    "\n",
    "# 새로운 특성 추가: 'bhk' 열 생성 (침실 수)\n",
    "df3['bhk'] = df3['size'].apply(lambda x: int(x.split(' ')[0]))\n",
    "\n",
    "# 'bhk' 열의 고유한 값 확인\n",
    "df3.bhk.unique()\n",
    "\n",
    "# 'total_sqft' 열의 값 변환 함수\n",
    "def is_float(x):\n",
    "    try:\n",
    "        float(x)\n",
    "    except:\n",
    "        return False\n",
    "    return True\n",
    "\n",
    "# 'total_sqft' 열의 값 변환 함수 적용\n",
    "df3[~df3['total_sqft'].apply(is_float)].head(10)\n",
    "\n",
    "# 'total_sqft' 열의 값 변환 함수 정의\n",
    "def convert_sqft_to_num(x):\n",
    "    tokens = x.split('-')\n",
    "    if len(tokens) == 2:\n",
    "        return (float(tokens[0]) + float(tokens[1])) / 2\n",
    "    try:\n",
    "        return float(x)\n",
    "    except:\n",
    "        return None\n",
    "\n",
    "# 'total_sqft' 열 값 변환 적용 및 결측치 제거\n",
    "df4 = df3.copy()\n",
    "df4.total_sqft = df4.total_sqft.apply(convert_sqft_to_num)\n",
    "df4 = df4[df4.total_sqft.notnull()]\n",
    "df4.head(2)\n",
    "\n",
    "# 'price_per_sqft' 열 추가\n",
    "df5 = df4.copy()\n",
    "df5['price_per_sqft'] = df5['price'] * 100000 / df5['total_sqft']\n",
    "df5.head()\n",
    "\n",
    "# 'price_per_sqft' 열의 통계 정보 확인\n",
    "df5_stats = df5['price_per_sqft'].describe()\n",
    "df5_stats\n",
    "\n",
    "# 데이터셋 저장\n",
    "df5.to_csv(\"bhp.csv\", index=False)\n",
    "\n",
    "# 'location' 열 값 전처리: 불필요한 공백 제거\n",
    "df5.location = df5.location.apply(lambda x: x.strip())\n",
    "\n",
    "# 'location' 열의 값 빈도 확인\n",
    "location_stats = df5['location'].value_counts(ascending=False)\n",
    "location_stats\n",
    "\n",
    "# 'location' 열의 총 데이터 개수 확인\n",
    "location_stats.values.sum()\n",
    "\n",
    "# 빈도가 일정 수준 이하인 'location' 값을 'other'로 변환\n",
    "location_stats_less_than_10 = location_stats[location_stats <= 10]\n",
    "df5.location = df5.location.apply(lambda x: 'other' if x in location_stats_less_than_10 else x)\n",
    "\n",
    "# 변환 후의 'location' 열의 고유한 값 개수 확인\n",
    "len(df5.location.unique())\n",
    "\n",
    "# 'bhk' 당 평균 가격을 기준으로 이상치 제거\n",
    "df6 = df5[~(df5.total_sqft / df5.bhk < 300)]\n",
    "df6.shape\n"
   ]
